{
	"useTabs": true,
	"tabWidth": 4,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 140,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte",
				"singleQuote": false
			}
		},
		{
			"files": ["*.ts", "*.js"],
			"options": {
				"singleQuote": false
			}
		}
	]
}
